---
import SocialButton from '@/components/SocialButton.astro';
import Layout from '@/layouts/Layout.astro';
import { TEATOV_SOCIALS } from '@/lib/socials';
---

<Layout title="About">
  <section class="section">
    <ul class="flex gap-2">
      {
        TEATOV_SOCIALS.map((socialLink) => (
          <li>
            <SocialButton socialLink={socialLink} />
          </li>
        ))
      }
    </ul>
  </section>
</Layout>
